#include <iostream>
#include <cstring>

using namespace std;

int main() {
    const int N = 10;
    char str[N];

    cout << "Nhap chuoi co do dai nho hon " << N << ": ";
    cin >> str;
    cout << "Chuoi nhap vao: " << str << "\n";

    return 0;
}

/*
Output:
Nhap chuoi co do dai nho hon 10: 
*** stack smashing detected ***: terminated
*/
