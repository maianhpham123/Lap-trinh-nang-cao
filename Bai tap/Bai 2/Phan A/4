# include <bits/stdc++.h>
using namespace std;

struct date{
    int ngay, thang, nam;
    char ch;
}

date ngay_hom_nay {
    cin >> ngay_hom_nay.ngay >> ngay_hom_nay.ch >> ngay_hom_nay.thang >>  ngay_hom_nay.ch >> ngay_hom_nay.nam;
}

date ngay_tiep_theo (date ngay_hom_nay, int k) {
    ngay_tiep_theo.ngay = ngay_hom_nay.ngay + k;

    switch (ngay_hom_nay.thang) {
        case 1:
            if (ngay_tiep_theo.ngay <= 0) {
                ngay_tiep_theo.ngay = 31 + ngay_tiep_theo.ngay;
                ngay_tiep_theo.thang = 12;
                ngay_tiep_theo.nam = ngay_hom_nay.nam - 1;
            }

            else if (ngay_tiep_theo.ngay > 0 && ngaytieptheo.ngay <= 31) {
                ngay_tiep_theo.thang = 1;
                ngay_tiep_theo.nam = ngay_hom_nay.nam;
            }

            else if (ngay_tiep_theo.ngay > 31) {
                ngay_tiep_theo.ngay = ngay_tiep_theo.ngay - 31;
                ngay_tiep_theo.thang = 2;
                ngay_tiep_theo.nam = ngay_hom_nay.nam;
            }
        
        case 3:
            if (ngay_tiep_theo.ngay <= 0) {
                ngay_tiep_theo.ngay = 28 + ngay_tiep_theo.ngay;
                ngay_tiep_theo.thang = 2;
                ngay_tiep_theo.nam = ngay_hom_nay.nam;
            }

            else if (ngay_tiep_theo.ngay > 0 && ngaytieptheo.ngay <= 31) {
                ngay_tiep_theo.thang = 3;
                ngay_tiep_theo.nam = ngay_hom_nay.nam;
            }

            else if (ngay_tiep_theo.ngay > 31) {
                ngay_tiep_theo.ngay = ngay_tiep_theo.ngay - 31;
                ngay_tiep_theo.thang = 4;
                ngay_tiep_theo.nam = ngay_hom_nay.nam;
            }
        
        case 5:
            if (ngay_tiep_theo.ngay <= 0) {
                ngay_tiep_theo.ngay = 30 + ngay_tiep_theo.ngay;
                ngay_tiep_theo.thang = 4;
                ngay_tiep_theo.nam = ngay_hom_nay.nam;
            }

            else if (ngay_tiep_theo.ngay > 0 && ngaytieptheo.ngay <= 31) {
                ngay_tiep_theo.thang = 5;
                ngay_tiep_theo.nam = ngay_hom_nay.nam;
            }

            else if (ngay_tiep_theo.ngay > 31) {
                ngay_tiep_theo.ngay = ngay_tiep_theo.ngay - 31;
                ngay_tiep_theo.thang = 6;
                ngay_tiep_theo.nam = ngay_hom_nay.nam;
            }
        
        case 7:
            if (ngay_tiep_theo.ngay <= 0) {
                ngay_tiep_theo.ngay = 30 + ngay_tiep_theo.ngay;
                ngay_tiep_theo.thang = 6;
                ngay_tiep_theo.nam = ngay_hom_nay.nam;
            }

            else if (ngay_tiep_theo.ngay > 0 && ngaytieptheo.ngay <= 31) {
                ngay_tiep_theo.thang = 7;
                ngay_tiep_theo.nam = ngay_hom_nay.nam;
            }

            else if (ngay_tiep_theo.ngay > 31) {
                ngay_tiep_theo.ngay = ngay_tiep_theo.ngay - 31;
                ngay_tiep_theo.thang = 8;
                ngay_tiep_theo.nam = ngay_hom_nay.nam;
            }

        case 8:
            if (ngay_tiep_theo.ngay <= 0) {
                ngay_tiep_theo.ngay = 31 + ngay_tiep_theo.ngay;
                ngay_tiep_theo.thang = 7;
                ngay_tiep_theo.nam = ngay_hom_nay.nam;
            }

            else if (ngay_tiep_theo.ngay > 0 && ngaytieptheo.ngay <= 31) {
                ngay_tiep_theo.thang = 8;
                ngay_tiep_theo.nam = ngay_hom_nay.nam;
            }

            else if (ngay_tiep_theo.ngay > 31) {
                ngay_tiep_theo.ngay = ngay_tiep_theo.ngay - 31;
                ngay_tiep_theo.thang = 9;
                ngay_tiep_theo.nam = ngay_hom_nay.nam;
            }
        
        case 10:
            if (ngay_tiep_theo.ngay <= 0) {
                ngay_tiep_theo.ngay = 30 + ngay_tiep_theo.ngay;
                ngay_tiep_theo.thang = 9;
                ngay_tiep_theo.nam = ngay_hom_nay.nam;
            }

            else if (ngay_tiep_theo.ngay > 0 && ngaytieptheo.ngay <= 31) {
                ngay_tiep_theo.thang = 10;
                ngay_tiep_theo.nam = ngay_hom_nay.nam;
            }

            else if (ngay_tiep_theo.ngay > 31) {
                ngay_tiep_theo.ngay = ngay_tiep_theo.ngay - 31;
                ngay_tiep_theo.thang = 11;
                ngay_tiep_theo.nam = ngay_hom_nay.nam;
            }

        case 12:
            if (ngay_tiep_theo.ngay <= 0) {
                ngay_tiep_theo.ngay = 30 + ngay_tiep_theo.ngay;
                ngay_tiep_theo.thang = 11;
                ngay_tiep_theo.nam = ngay_hom_nay.nam;
            }

            else if (ngay_tiep_theo.ngay > 0 && ngaytieptheo.ngay <= 31) {
                ngay_tiep_theo.thang = 12;
                ngay_tiep_theo.nam = ngay_hom_nay.nam;
            }

            else if (ngay_tiep_theo.ngay > 31) {
                ngay_tiep_theo.ngay = ngay_tiep_theo.ngay - 31;
                ngay_tiep_theo.thang = 1;
                ngay_tiep_theo.nam = ngay_hom_nay.nam + 1;
            }

            
    }

}
