# include <bits/stdc++.h>
using namespace std;

bool isPrime (int n){
  if (n <= 1) return false;
  else if (n == 2) return true;
  else if (n >= 3) {
    for (int i = 2; i < n; i++) {
      if (n % i == 0) return false;
    }
    return true;
  }
}

int main()
{
  int n;
  cin >> n;
  cout << isPrime (n) << endl;
}
