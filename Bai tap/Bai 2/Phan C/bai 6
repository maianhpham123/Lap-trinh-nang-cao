#include <bits/stdc++.h>
using namespace std;

int main()
{
     int n;
     cin >> n;
     int k = 0;
     for (int i = 1; i <= n; i++) {
          for (int j = 1; j <= n; j++) {
               if ((j + k) % n == 0) cout << n << " ";
               else cout << (j + k) % n  << " ";
          }
          cout << endl;
          k ++;
     }
}
